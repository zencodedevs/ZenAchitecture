{
  "name": "zen-architecture.ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --open",
    "start:dev": "echo 'Starting (Dev)...' && ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build:ssr": "ng run ZenAchitecture.WebUI:server:dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-prod-no-cache-with-version": "node ./replace.build.js && ng build  --configuration=production && node ./versioning.build.js",
    "build-prod-no-cache-with-version-qa": "node ./replace.build.js && ng build --configuration=qa && node ./versioning.build.js",
    "build-prod-no-cache-with-version-dev": "node ./replace.build.js && ng build --configuration=dev && node ./versioning.build.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.1",
    "@angular/cdk": "^16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/localize": "^16.1.1",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "guid-typescript": "^1.0.9",
    "ngx-permissions": "^15.0.1",
    "ngx-progressbar": "^11.0.0",
    "ngx-toastr": "^17.0.2",
    "oidc-client": "^1.11.5",
    "replace-in-file": "^7.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zencode-configuration-audit": "^16.0.2",
    "zencode-dialog-asy": "^16.0.1",
    "zencode-dialog-obs": "^16.0.2",
    "zencode-http-builder": "^16.0.2",
    "zencode-soc-share": "^16.0.1",
    "zencode-style-provider": "^16.0.2",
    "zencode-tracker-provider": "^16.0.1",
    "zencode-utilities": "^16.0.1",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "^16.1.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.1.0",
    "typescript": "~5.1.3"
  }
}
